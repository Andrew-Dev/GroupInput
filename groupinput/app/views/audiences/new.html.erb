<div class="col-md-4"></div>
<div class="col-md-4">
<h1>New Audience</h1>
<% flash.each do |name, msg| -%>
	<p class="alert alert-<%=name%>"><%= msg %></p>
<% end %>
<%= form_for @audience, url: {action: "create"} do |f| %>
    <%= f.label :audience_title %>
    <p><%= f.text_field :title, autofocus: true, class: 'form-control' %></p>
    <p><%= f.submit "Create Audience", class: 'btn btn-primary' %></p>
    <p><%= link_to('Back to Audiences',audiences_path) %></p>
<% end %>
</div>
<div class="col-md-4"></div>

